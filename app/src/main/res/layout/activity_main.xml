<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/editTextNumber12"
        android:layout_width="145dp"
        android:layout_height="28dp"
        android:ems="4"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="216dp" />

    <EditText
        android:id="@+id/Renteomkostninger"
        android:layout_width="145dp"
        android:layout_height="28dp"
        android:ems="4"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="499dp" />

    <EditText
        android:id="@+id/Bruttofortjeneste"
        android:layout_width="145dp"
        android:layout_height="28dp"
        android:ems="4"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.463"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="172dp" />

    <EditText
        android:id="@+id/Afskrivninger"
        android:layout_width="145dp"
        android:layout_height="28dp"
        android:ems="4"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="400dp" />

    <EditText
        android:id="@+id/Indtjeningsbidrag"
        android:layout_width="145dp"
        android:layout_height="28dp"
        android:ems="4"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="356dp" />

    <EditText
        android:id="@+id/Vareforbrug"
        android:layout_width="145dp"
        android:layout_height="28dp"
        android:ems="4"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.541"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="133dp" />

    <EditText
        android:id="@+id/Omsætning"
        android:layout_width="145dp"
        android:layout_height="28dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="81dp"
        android:ems="4"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/switch1"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/Markedsføringsbidrag"
        android:layout_width="145dp"
        android:layout_height="28dp"
        android:ems="4"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.154"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="261dp" />

    <EditText
        android:id="@+id/edit"
        android:layout_width="145dp"
        android:layout_height="28dp"
        android:ems="4"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="450dp" />
    <TextView
        android:text="Øvrige kapacitetsomkostninger"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:id="@+id/textView7" tools:layout_editor_absoluteY="306dp"
        tools:layout_editor_absoluteX="22dp" tools:ignore="MissingConstraints"/>
    <TextView
        android:text="Markedsføringsomkostninger"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:id="@+id/textView5" tools:layout_editor_absoluteY="216dp"
        tools:layout_editor_absoluteX="23dp" tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Resultatopgørelse"
        android:textColor="#ff000000"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="99dp"
        android:layout_height="27dp"
        android:layout_marginEnd="16dp"
        android:background="#6200EE"
        android:text="Visualiser budget"
        android:textColor="#ffffffff"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/button2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="661dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="150dp"
        android:layout_height="27dp"
        android:background="#6200EE"
        android:text=" + Generer BalanceBudget"
        android:textColor="#ffffffff"
        android:textSize="10sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="661dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="28dp"
        android:text="Omsætning"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="23dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:text="Vareforbrug"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="30dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="28dp"
        android:text="Bruttofortjeneste"
        android:textColor="#ff000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:text="Markedsføringsbidrag"
        android:layout_width="wrap_content"
        android:textSize="12sp"
        android:textColor="#ff000000"
        android:layout_height="wrap_content" android:id="@+id/textView6" tools:layout_editor_absoluteY="261dp"
        tools:layout_editor_absoluteX="23dp" tools:ignore="MissingConstraints"/>
    <TextView
        android:text="Indtjeningsbidrag"
        android:layout_width="wrap_content"
        android:textSize="12sp"
        android:textColor="#ff000000"
        android:layout_height="wrap_content" android:id="@+id/textView8" tools:layout_editor_absoluteY="356dp"
        tools:layout_editor_absoluteX="22dp" tools:ignore="MissingConstraints"/>
    <TextView
        android:text="Afskrivninger"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:id="@+id/textView9" tools:layout_editor_absoluteY="400dp"
        tools:layout_editor_absoluteX="22dp" tools:ignore="MissingConstraints"/>
    <TextView
        android:text="Resultat før renter"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:textColor="#ff000000"
        android:layout_height="wrap_content" android:id="@+id/textView10" tools:layout_editor_absoluteY="450dp"
        tools:layout_editor_absoluteX="22dp" tools:ignore="MissingConstraints"/>
    <TextView
        android:text="Renteomkostninger"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:id="@+id/textView13" tools:layout_editor_absoluteY="499dp"
        tools:layout_editor_absoluteX="22dp" tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:text="Resultat"
        android:textColor="#ff000000"
        android:textSize="17sp"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="31dp" />

    <Switch
        android:id="@+id/switch1"
        android:layout_width="105dp"
        android:layout_height="39dp"
        android:layout_marginStart="100dp"
        android:text="Beløb angives i kr."
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/textView"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/editTextNumber3"
        android:layout_width="145dp"
        android:layout_height="28dp"
        android:ems="7"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="306dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="77dp"
        android:layout_height="26dp"
        android:layout_marginStart="24dp"


        android:background="#6200EE"
        android:text="Eksporter"
        android:textColor="#ffffffff"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/button2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="661dp" />


</androidx.constraintlayout.widget.ConstraintLayout>